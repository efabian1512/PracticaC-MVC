@model  IEnumerable< AJAX.Controllers.Persona>

<table class="table table-striped">

    <thead>
        <tr>
            <th> Nombre</th>
            <th>Edad</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var persona in Model)
        {
            <tr>
                <td>@persona.Nombre</td> 
                <td>@persona.Edad</td>
            </tr>
        }
    </tbody>
</table>
